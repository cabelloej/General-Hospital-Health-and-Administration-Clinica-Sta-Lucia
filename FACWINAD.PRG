DEFI WIND WINDADM1 FROM 06,05 TO 20,75 ;
                   TITLE " PACIENTES CON CUENTAS DE INGRESOS HOSPITALARIOS " ;
                   FOOTER " ESC=Selec/Salir ";
                   DOUBLE NOFLOAT NOZOOM NOGROW COLOR SCHEME 10
SELECT ADMADM
SET ORDER TO ADMADM1
ACTI WIND WINDADM1
BROWSE FIELDS CODPAC:H="CODIGO", TIPPAC:H="TIPO",UBICACION:H="UBIC.",CAMA:H="CAMA",;
              WNOMBRE=SUBSTR((ALLTRIM(PAPELLIDO)+" "+ALLTRIM(SAPELLIDO)+" "+ALLTRIM(PNOMBRE)+" "+ALLTRIM(SNOMBRE)),1,35):H="NOMBRE                            ",;
              CUENTA:H="CUENTA";
              NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE IN WINDOW WINDADM1
              *NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE REST SAVE IN WINDOW WINDADM1
STORE SUBSTR(CUENTA,4,7) TO WNUMREF
RELE WIND WINDADM1
RETURN
