SELECT SYSMED
DEFI WIND BRMED FROM LIBRMED,CIBRMED TO LIBRMED+10,CIBRMED+40 ;
                TITLE " MEDICOS " ;
                FOOTER " ESC = Seleccionar ";
                DOUBLE NOFLOAT NOZOOM NOGROW SHADOW COLOR SCHEME 10
ACTI WIND BRMED
BROWSE FIELDS MEDICO:H="MEDICO", NOMBRE:H="NOMBRE";
              NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE REST SAVE IN WINDOW BRMED
RETURN

