SELECT ALMART
SET ORDER TO ALMART2
DEFI WIND BRSER FROM LIBRART,CIBRART TO LIBRART+10,CIBRART+75 ;
                TITLE " ARTICULOS " ;
                FOOTER " ESC=Seleccionar ";
                DOUBLE NOFLOAT NOZOOM NOGROW SHADOW COLOR SCHEME 10
ACTI WIND BRSER
BROWSE FIELDS CODIGO:H="CODIGO ITEM", DESCRI:H="DESCRIPCION", UNIMED:H="UNIDAD";
              NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE REST SAVE IN WINDOW BRSER
RELEASE WIND BRSER
SET ORDER TO ALMART
RETURN
