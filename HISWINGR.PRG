SELECT HISGRU
GO TOP
DEFI WIND BRGRU FROM LIBRGRU,CIBRGRU TO LIBRGRU+10,CIBRGRU+40 ;
                TITLE " ACTIVIDADES DE LA HISTORIA " ;
                FOOTER " ESC = Seleccionar ";
                DOUBLE NOFLOAT NOZOOM NOGROW SHADOW COLOR SCHEME 10
ACTI WIND BRGRU
BROWSE FIELDS CODGRU:H="ACTIVIDAD", DESGRU:H="DESCRIPCION";
              NOAPPEND NODELETE NOEDIT NOMENU NOOPTIMIZE REST SAVE IN WINDOW BRGRU
              *FOR UBIGRU=WUSERUBI.OR.UBIGRU=SPACE(3)
RETURN

