IF SUBSTR(SERDCGE.NUMERO,1,3)="SOL"
   RETURN
ENDIF
SELECT 20
USE FACDCGE INDEX FACDCGE2
SELECT 21
USE FACDCDE INDEX FACDCDE
* LE COLOCA EL AREA Y AUTOR DEL COMPROBANTE A LA FACTURA
* LA REFERENCIA DEL COMPROBANTE ES LA MISMA DE LA FACTURA (SOLICITUD)
SELECT FACDCGE
SEEK SERDCGE.REFERENCIA
IF FOUND()
   SELECT FACDCDE
   SEEK FACDCGE.NUMERO
   *** REVERSA EL EJECUTOR
   DO WHILE .NOT.EOF().AND.NUMERO=FACDCGE.NUMERO
      IF RECLOC()
         REPLACE EJECUTOR WITH SPACE(8)
         FLUSH
         UNLOCK ALL
      ENDIF
      SELECT FACDCDE
      SKIP
   ENDDO
ENDIF
***
SELECT SERDCDE
SEEK SERDCGE.NUMERO
DO WHILE .NOT.EOF().AND.NUMERO=SERDCGE.NUMERO
   IF EJECUTOR<>SPACE(8)
      SELECT FACDCDE
      SEEK FACDCGE.NUMERO
      DO WHILE.NOT.EOF().AND.NUMERO=FACDCGE.NUMERO
        IF RENGLON=SERDCDE.RENGLON
           IF RECLOC()
              REPLACE EJECUTOR WITH SERDCDE.EJECUTOR
              FLUSH
              UNLOCK ALL
              EXIT
           ENDIF
        ENDIF
        SELECT FACDCDE
        SKIP
      ENDDO
   ENDIF
   SELECT SERDCDE
   SKIP
ENDDO
RETURN

